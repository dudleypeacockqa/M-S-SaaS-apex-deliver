[run]
source = backend/app
parallel = True
concurrency = thread
plugins =
    coverage_pytracer_patch
omit =
    backend/venv/*
    */sage_oauth_service.py
    */quickbooks_oauth_service.py
    */netsuite_oauth_service.py
    */xero_oauth_service.py
    */s3_storage_service.py
    app/api/routes/pmi.py
    app/api/routes/voice.py
    app/services/pmi_*.py
    app/services/voice_service.py
    app/services/conversation_engine_service.py
    app/tasks/pmi_*.py
    app/tasks/voice_tasks.py

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
